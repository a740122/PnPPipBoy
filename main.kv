                    text:'Finish'
                    BoxLayout:
                        padding:20,200
                        PipButton:
                            text:'Finished Creating Character'
                            on_press:
                                root.playerCharacter.update()
                                root.saveCharacter()
                                root.children[0].current='PipBoy'
        Screen:
            name:'PipBoy'
            on_enter:
                #root.playerCharacter=root.reloadCharacter()
                STLabel.updateLabel(root, 'ST', 0)
